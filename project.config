[project]
	description = Vector Packet Processor
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/mido-gerrithub-sync
[access "refs/for/refs/*"]
	read = group Anonymous Users
	read = group user/mido-gerrithub-sync
	push = group Registered Users
	push = group user/mido-gerrithub-sync
	label-Code-Review = -2..+2 group user/mido-gerrithub-sync
	label-Verified = -1..+1 group user/mido-gerrithub-sync
	rebase = group midonet
	rebase = group user/mido-gerrithub-sync
	removeReviewer = group midonet
	removeReviewer = group user/mido-gerrithub-sync
	submit = group user/mido-gerrithub-sync
[access "refs/heads/*"]
	read = group Anonymous Users
	read = group user/mido-gerrithub-sync
	create = group midonet
	create = group user/mido-gerrithub-sync
	push = +force group midonet
	push = +force group user/mido-gerrithub-sync
	pushMerge = group midonet
	pushMerge = group user/mido-gerrithub-sync
	abandon = group midonet
	label-Code-Review = -2..+2 group midonet
	label-Verified = -1..+1 group midonet
	label-Verified = -1..+1 group midonet-bots
	labelAs-Code-Review = -2..+2 group midonet
	labelAs-Verified = -1..+1 group midonet
	rebase = group midonet
	publishDrafts = group midonet
	viewDrafts = group midonet
	deleteDrafts = group midonet
	submit = group midonet
[access "refs/tags/*"]
	push = +force group midonet
	push = +force group user/mido-gerrithub-sync
	pushTag = +force group midonet
	pushTag = +force group user/mido-gerrithub-sync
	pushSignedTag = group midonet
	pushSignedTag = group user/mido-gerrithub-sync
[receive]
	requireSignedOffBy = true
	requireChangeId = true
