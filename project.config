[project]
	description = Vector Packet Processor
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/mido-gerrithub-sync
[access "refs/for/refs/*"]
	read = group midonet-core
	read = group user/mido-gerrithub-sync
	push = group midonet-core
	push = group user/mido-gerrithub-sync
	label-Code-Review = -2..+2 group midonet-core
	label-Code-Review = -2..+2 group user/mido-gerrithub-sync
	label-Verified = -1..+1 group midonet-core
	label-Verified = -1..+1 group user/mido-gerrithub-sync
	rebase = group midonet-core
	rebase = group user/mido-gerrithub-sync
	removeReviewer = group midonet-core
	removeReviewer = group user/mido-gerrithub-sync
	submit = group midonet-core
	submit = group user/mido-gerrithub-sync
[access "refs/heads/*"]
	read = group midonet-core
	read = group user/mido-gerrithub-sync
	create = group user/mido-gerrithub-sync
	push = group midonet-core
	push = +force group user/mido-gerrithub-sync
	pushMerge = group midonet-core
	pushMerge = group user/mido-gerrithub-sync
[access "refs/tags/*"]
	push = group midonet-core
	push = +force group user/mido-gerrithub-sync
	pushTag = group midonet-core
	pushTag = group user/mido-gerrithub-sync
	pushSignedTag = group midonet-core
	pushSignedTag = group user/mido-gerrithub-sync
